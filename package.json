{
  "private": true,
  "scripts": {
    "start": "umi dev",
    "build": "umi build",
    "postinstall": "umi generate tmp",
    "prettier": "prettier --write '**/*.{js,jsx,tsx,ts,less,md,json}'",
    "test": "umi-test",
    "test:coverage": "umi-test --coverage"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,jsx,less,md,json}": [
      "prettier --write"
    ],
    "*.ts?(x)": [
      "prettier --parser=typescript --write"
    ]
  },
  "dependencies": {
    "@ant-design/icons": "^4.3.0",
    "@ant-design/pro-form": "^1.14.0",
    "@ant-design/pro-layout": "^6.11.0",
    "@ant-design/pro-table": "^2.22.0",
    "@antv/data-set": "^0.11.7",
    "@umijs/route-utils": "^1.0.33",
    "antd": "^4.10.0",
    "classnames": "^2.2.6",
    "echarts": "^4.9.0",
    "echarts-for-react": "^2.0.16",
    "lodash": "^4.17.11",
    "moment": "^2.24.0",
    "omit.js": "^2.0.2",
    "path-to-regexp": "^6.2.0",
    "prop-types": "^15.7.2",
    "qs": "^6.9.0",
    "swiper": "^6.4.8",
    "umi": "^3.3.7",
    "umi-request": "^1.3.5"
  },
  "devDependencies": {
    "@types/react": "^16.9.0",
    "@types/react-dom": "^16.9.0",
    "@umijs/preset-react": "^1.7.13",
    "@umijs/test": "^3.3.7",
    "lint-staged": "^10.0.7",
    "prettier": "^2.2.0",
    "react": "17.x",
    "react-dom": "17.x",
    "typescript": "^4.1.2",
    "yorkie": "^2.0.0"
  }
}
